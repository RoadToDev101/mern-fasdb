{
  "name": "mern-fasdb",
  "version": "1.0.0",
  "description": "Fastener Database for Simpson Strong-Tie",
  "main": "server.js",
  "scripts": {
    "install-dependencies": "npm run install-client && npm install",
    "install-client": "cd client && npm install",
    "server": "nodemon server.js --ignore client",
    "client": "npm start --prefix client",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "build-client": "cd client && npm run build",
    "setup-production": "npm run install-client && npm run build-client && npm install"
  },
  "keywords": [
    "fastener",
    "database",
    "Simpson Strong-Tie"
  ],
  "author": "sopham@strongtie.com",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "latest",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.0",
    "mongodb": "^5.5.0",
    "mongoose": "^7.1.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.2",
    "validator": "^13.9.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "concurrently": "latest",
    "nodemon": "^2.0.21"
  }
}
